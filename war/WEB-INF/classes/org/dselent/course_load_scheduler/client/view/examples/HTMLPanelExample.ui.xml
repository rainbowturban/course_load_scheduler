<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<p>
		<font size="28">Hello</font><br/>
		<a href = "https://www.w3schools.com/tags/ref_byfunc.asp">Link to HTML Tags</a><br/>
		<g:Button styleName="{style.important}" ui:field="myButton" text="Button Text" /><br/>
		<g:Button ui:field="myButton2">Button 2 Text</g:Button>
		</p>
	</g:HTMLPanel>
</ui:UiBinder> 